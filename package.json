{
  "name": "frontigrade",
  "version": "0.0.1",
  "description": "Frontigrade, web user interface for Cloudigrade",
  "author": "Red Hat",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/cloudigrade/frontigrade.git"
  },
  "bugs": {
    "url": "https://gitlab.com/cloudigrade/frontigrade/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/common/helpers.js",
      "!src/components/app.js",
      "!src/index.js",
      "!src/services/index.js",
      "!src/setupTests.js",
      "!src/redux/index.js",
      "!src/redux/store.js",
      "!src/redux/middleware/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "api:dev": "sh ./scripts/dev.sh -p 5000 -d \"$(pwd)/src/services\"",
    "api:oc": "sh ./scripts/oc.sh",
    "api:oc-update": "sh ./scripts/oc.sh -u",
    "build:clean": "rm -rf -- \"$(pwd)\"/build",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/index.scss -o src/styles/.css",
    "build:js": "react-scripts build",
    "build:rh": "sh -ac 'export REACT_APP_RH_BRAND=true && run-s -l build:clean build:css build:js'",
    "build": "run-s -l build:clean build:css build:js",
    "locale:gettext": "node ./scripts/gettext.js",
    "start:js": "react-scripts start",
    "start:oc": "sh -ac '. ./.env.oc; run-p -l watch:css start:js api:oc'",
    "start:review": "sh -ac '[[ -e ./.env.local ]] && . ./.env.local; . ./.env.review; [[ ! -z $API_HOST ]] && run-p -l watch:css start:js || echo \"API_HOST is not defined, review the README for review serve directions.\"'",
    "start": "run-p -l watch:css start:js api:dev",
    "test:dev": "run-s test:lint-js test:local",
    "test:lint-js": "eslint --ext=json --ext=js --ext=jsx src ./public/locales",
    "test:ci": "CI=true; react-scripts test --env=jsdom --coverage",
    "test:clearCache": "react-scripts test --clearCache",
    "test:local": "react-scripts test --env=jsdom",
    "test": "run-s test:lint-js test:ci",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/index.scss -o src/styles/.css --watch --recursive"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "create-react-app": "^1.5.2",
    "detect-browser": "^3.0.1",
    "i18next": "^11.3.6",
    "i18next-chained-backend": "^0.1.2",
    "i18next-localstorage-backend": "^1.1.4",
    "i18next-xhr-backend": "^1.5.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.3",
    "node-sass-chokidar": "^1.3.3",
    "patternfly": "3.55.0",
    "patternfly-react": "^2.24.3",
    "react": "^16.5.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.1",
    "react-i18next": "^7.8.0",
    "react-infinite-scroller": "^1.2.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "gettext-extractor": "^3.3.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "moxios": "^0.4.0",
    "prettier": "^1.14.2",
    "redux-mock-store": "^1.5.3"
  },
  "resolutions": {
    "**/jest": "23.6.0"
  }
}
